    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  }
